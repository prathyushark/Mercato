<div class="section">
  <ul id="ulOrdersCollapsible" class="collapsible popout" data-collapsible="accordion">
    <% @subscriptions['subscriptions'].each do |subscription| %>
      <li>
        <div class="collapsible-header" id="divOrderCollapsibleHeader-<%= subscription['subscription_id'] %>">
          <span class="left">Subscription <%= subscription['subscription_id'] %></span>
          <span class="right">Subscribed on <%= subscription['created_at'].split('T')[0] %></span>
        </div>
        <div class="collapsible-body">

                  <table class="highlight">
                    <thead>
                    <tr>
                      <th data-field="name">Product Name</th>
                      <th data-field="quantity">Quantity</th>
                      <th data-field="status">Status</th>
                      <th data-field="frequency">Frequency</th>
                      <th data-field="duedate">Next Due Date</th>

                      <th></th>
                    </tr>
                    </thead>

                    <tbody>
                          <td id="tdProductName-<%= subscription['subscription_id'] %>"><%= subscription['product']['product_name'] %></td>
                          <td id="tdQuantity-<%= subscription['sNaubscription_id'] %>"><%= subscription['quantity'] %></td>
                          <td id="tdStatus-<%= subscription['subscription_id'] %>"><%= subscription['status'] %></td>
                          <td id="tdFrequency-<%= subscription['subscription_id'] %>"><%= subscription['frequency_in_days'] %></td>
                          <td id="tdNextDueDate-<%= subscription['subscription_id'] %>"><%= subscription['next_due_date'] %></td>
                          <td>
                          <a id="tdSkipDueDate-<%= subscription['subscription_id'] %>" class='waves-effect waves-light btn'
                          onclick="MERCATO.SubscriptionsList.skipDueDate(this)">Skip Next Delivery
                          </a>
                          </td>

                        </tr>
                    </tbody>
                  </table>
</div>
      </li>
    <% end %>
  </ul>
</div>
